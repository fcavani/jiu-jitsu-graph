type End {
    name: string
}

type Person {
    name: string
    country: string
}

type Link {
    url: string
    last_seen: dateTime
    last_modification: dateTime
    created_at: dateTime
    mime: string
}

type Move {
    id: string
    name: [string]
    description: string
    class: Basics
    points: int
    author: Person
    situation: string
    lapela: bool
    variations: [Move]
    medias: [Link]
}

type Basics {
    name: string
    is: Basics
    description: string
    movements: Move
}

type Position {
    name: string
    alternative_name: [string]
    description: string
    type: Basics
    fall: [Position]
    guard: [Position]
    pass: [Position]
    sweep: [Position]
    side_control: [Position]
    back: [Position]
    mount: [Position]
    submission: Position
}

type Start {
    name: string
    begin: [Position]
}

id: string @index(term) .
